{
  "expo": {
    "name": "SaySup",
    "slug": "say-sup",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7e95f7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gravity.saysup"
    },
    "android": {
      "package": "com.gravity.saysup"
    },
    "description": "Open Source Messaging App",
    "githubUrl": "https://github.com/g-ravity/saysup"
  }
}